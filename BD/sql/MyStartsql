CREATE TABLE DAI_t(
X NUMBER (3) PRIMARY KEY,
S VARCHAR2(50)
);

INSERT INTO DAI_t(X, S) VALUES('11','ABC'); 
INSERT INTO DAI_t(X, S) VALUES('22','BCD');
INSERT INTO DAI_t(X, S) VALUES('33','ABC');
COMMIT;

SELECT * FROM DAI_t;

UPDATE DAI_t SET S = 'CHANGE' WHERE X = 11;
UPDATE DAI_t SET S = 'CHANGE2' WHERE X = 22;
COMMIT;

SELECT MIN (X) FROM DAI_t;

DELETE FROM DAI_t WHERE X = 33;
ROLLBACK;

CREATE TABLE DAI_T_child(
ID NUMBER(3) PRIMARY KEY,
INFO VARCHAR2(50),
CONSTRAINT FK_ FOREIGN KEY (id)
REFERENCES DAI_t(X)
);

INSERT INTO DAI_t_child(ID, INFO) VALUES('11','ANDR'); 
INSERT INTO DAI_t_child(ID, INFO) VALUES('22','MAX');
INSERT INTO DAI_t_child(ID, INFO) VALUES('33','OLEG');
COMMIT;

SELECT X, INFO FROM DAI_t LEFT OUTER JOIN DAI_t_child
ON dai_t.x = dai_t_child.id;

SELECT X, INFO FROM DAI_t RIGHT OUTER JOIN DAI_t_child
ON dai_t.x = dai_t_child.id;

SELECT X, INFO FROM DAI_t JOIN DAI_t_child
ON dai_t.x = dai_t_child.id;

DROP TABLE DAI_t;
DROP TABLE DAI_t_child;

